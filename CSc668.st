Object subclass: #BinTree	instanceVariableNames: 'label left right'	classVariableNames: ''	poolDictionaries: ''	category: 'CSc668'!!BinTree methodsFor: 'as yet unclassified' stamp: 'sb 3/2/2016 16:54'!addLeftKid: kid	left := kid.! !!BinTree methodsFor: 'as yet unclassified' stamp: 'sb 3/2/2016 16:54'!addRightKid: kid	right := kid.! !!BinTree methodsFor: 'as yet unclassified' stamp: 'sb 3/2/2016 19:23'!getLabel	^ label.! !!BinTree methodsFor: 'as yet unclassified' stamp: 'sb 3/2/2016 19:32'!getLeftKid	^ left.! !!BinTree methodsFor: 'as yet unclassified' stamp: 'sb 3/2/2016 19:33'!getRightKid	^ right.! !!BinTree methodsFor: 'as yet unclassified' stamp: 'sb 3/2/2016 16:15'!initialize: aLabel	 label := aLabel.! !!BinTree methodsFor: 'as yet unclassified' stamp: 'sb 3/3/2016 14:05'!print	Transcript show: label.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!BinTree class	instanceVariableNames: ''!!BinTree class methodsFor: 'as yet unclassified' stamp: 'sb 3/2/2016 16:16'!new: label	^ super new initialize: label.! !Object subclass: #InorderTraversal	instanceVariableNames: 'binTree'	classVariableNames: ''	poolDictionaries: ''	category: 'CSc668'!!InorderTraversal methodsFor: 'as yet unclassified' stamp: 'sb 3/4/2016 12:38'!first	| nextLabel stack |	nextLabel _ binTree.	" Create a stack to hold passed over nodes "	stack _ Stack new.	" Iterate through the nodes until the left most is reached. "	[nextLabel notNil] whileTrue: [		" Store current node on the stack "		stack push: nextLabel.		nextLabel _ nextLabel getLeftKid.	].	" Return the last node on the stack "		^ stack pop.! !!InorderTraversal methodsFor: 'as yet unclassified' stamp: 'sb 3/3/2016 13:56'!initialize: aBinTree	binTree _ aBinTree.! !!InorderTraversal methodsFor: 'as yet unclassified' stamp: 'sb 3/4/2016 12:38'!next! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!InorderTraversal class	instanceVariableNames: ''!!InorderTraversal class methodsFor: 'as yet unclassified' stamp: 'sb 3/3/2016 13:57'!new: aBinTree	^ super new initialize: aBinTree.! !